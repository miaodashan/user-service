<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vplus.demo.userservice.mapper.UserMapper">
    <select id="selectUserByAge" parameterType="java.lang.Integer" resultType="com.vplus.demo.userservice.model.User">
        select id,username,name,age from `user` where age=#{age}
    </select>
</mapper>